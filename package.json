{
  "name": "fastly",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 4000",
    "dev:marketing": "next dev --turbopack -p 4000",
    "build": "next build",
    "build:marketing": "next build",
    "start": "next start",
    "start:marketing": "next start",
    "dev:app": "cd main-app && pnpm run dev",
    "build:app": "cd main-app && pnpm run build",
    "start:app": "cd main-app && pnpm run start",
    "prepare": "husky",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --fix",
    "format": "biome format . --write",
    "format:check": "biome format . --check",
    "lint:all": "pnpm run lint && cd main-app && pnpm run lint",
    "lint:fix:all": "pnpm run lint:fix && cd main-app && pnpm run lint:fix",
    "format:all": "pnpm run format && cd main-app && pnpm run format",
    "format:check:all": "pnpm run format:check && cd main-app && pnpm run format:check",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm dev:app\"",
    "clean": "rm -rf .next",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["biome lint --fix", "biome format --write"],
    "*.{json,css,md}": ["biome format --write"]
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@tanstack/react-query": "^5.90.2",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "axios": "1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "extract-zip": "^2.0.1",
    "install": "^0.13.0",
    "lucide-react": "^0.525.0",
    "mongoose": "^8.18.3",
    "motion": "^12.23.22",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20.19.19",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
